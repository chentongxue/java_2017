package sacred.alliance.magic.app.goods.behavior.param;

import sacred.alliance.magic.vo.RoleInstance;

public class RemovalPunchParam extends AbstractParam{

	private byte bagType ;
	private String goodsId ;
	private byte holeId ;
	
	public RemovalPunchParam(RoleInstance role) {
		super(role);
	}

	public byte getBagType() {
		return bagType;
	}

	public void setBagType(byte bagType) {
		this.bagType = bagType;
	}

	public String getGoodsId() {
		return goodsId;
	}

	public void setGoodsId(String goodsId) {
		this.goodsId = goodsId;
	}

	public byte getHoleId() {
		return holeId;
	}

	public void setHoleId(byte holeId) {
		this.holeId = holeId;
	}

	
}
