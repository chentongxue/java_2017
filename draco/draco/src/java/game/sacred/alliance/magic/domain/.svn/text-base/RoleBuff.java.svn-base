package sacred.alliance.magic.domain;

import java.util.Date;

public class RoleBuff {
	private String roleId;
	private short buffId;
	private String casterId;
	private int remainTime;//剩余时间
	private String extroInfo ;
	private int buffLevel ;
	private int intervalTime ; //间隔时间,单位毫秒
	private int buffSeries;//buff系列id
	private Date lastExecuteTime;//上次执行的时间
	private Date createTime;
	

	public Date getCreateTime() {
		return createTime;
	}
	public void setCreateTime(Date createTime) {
		this.createTime = createTime;
	}
	public String getRoleId() {
		return roleId;
	}
	public void setRoleId(String roleId) {
		this.roleId = roleId;
	}
	public short getBuffId() {
		return buffId;
	}
	public void setBuffId(short buffId) {
		this.buffId = buffId;
	}
	public String getCasterId() {
		return casterId;
	}
	public void setCasterId(String casterId) {
		this.casterId = casterId;
	}
	public int getRemainTime() {
		return remainTime;
	}
	public void setRemainTime(int remainTime) {
		this.remainTime = remainTime;
	}
	public Date getLastExecuteTime() {
		return lastExecuteTime;
	}
	public void setLastExecuteTime(Date lastExecuteTime) {
		this.lastExecuteTime = lastExecuteTime;
	}
	public String getExtroInfo() {
		return extroInfo;
	}
	public void setExtroInfo(String extroInfo) {
		this.extroInfo = extroInfo;
	}
	public int getBuffLevel() {
		return buffLevel;
	}
	public void setBuffLevel(int buffLevel) {
		this.buffLevel = buffLevel;
	}
	public int getIntervalTime() {
		return intervalTime;
	}
	public void setIntervalTime(int intervalTime) {
		this.intervalTime = intervalTime;
	}
	public int getBuffSeries() {
		return buffSeries;
	}
	public void setBuffSeries(int buffSeries) {
		this.buffSeries = buffSeries;
	}

	
	
}
