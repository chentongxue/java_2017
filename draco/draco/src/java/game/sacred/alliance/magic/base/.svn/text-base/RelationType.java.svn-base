package sacred.alliance.magic.base;

public enum RelationType {

	FRIEND(0,"好友"),
	BLACKLIST(1,"黑名单"),
	SPOUSE(2,"夫妻"),
	MASTERPRENTICE(3,"师徒"),
	TEAM(4,"组队"),
	;
	
	public int type;
	
	public String name;
	
	RelationType(int type, String name){
		this.type = type;
		this.name = name;
	}
	
	public int getType(){
		return type;
	}
	
}
