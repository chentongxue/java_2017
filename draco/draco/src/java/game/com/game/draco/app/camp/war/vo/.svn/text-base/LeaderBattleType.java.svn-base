package com.game.draco.app.camp.war.vo;

public enum LeaderBattleType {

	/*
	 * 0 系统自动攻击 1 获得声望 2 获得连杀 3 终止别人连杀
	 */

	system((byte) 0),
	camp_prestige((byte) 1), 
	continuous_Kill((byte) 2), 
	interrupt_kill((byte) 3), ;

	private final byte type;

	private LeaderBattleType(byte type) {
		this.type = type;
	}

	public byte getType() {
		return type;
	}
	
	

}
